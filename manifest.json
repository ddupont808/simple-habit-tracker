{
 	"manifest_version": 2,
 	"name": "Simple Habit Tracker",
 	"version": "1.0",
	"description": "A minimalistic habit tracker",
	"icons": { 
		"16": "images/icon16.png",
		"48": "images/icon48.png",
	   "128": "images/icon128.png"
	},
	"permissions": [
		"storage"
	],
  	"chrome_url_overrides" : {
    	"newtab": "index.html"
  	}
}
